{
  "expo": {
    "name": "hobbii_hooks",
    "slug": "hobbii_hooks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pp.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pp.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.hobbii_hooks"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/pp.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pp.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
